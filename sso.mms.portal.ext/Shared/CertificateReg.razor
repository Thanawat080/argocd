@page "/certificatereg"
@using Blazored.LocalStorage;
@using sso.mms.helper.PortalModel;
@using sso.mms.helper.Utility;
@using sso.mms.helper.ViewModels;
@using sso.mms.login.ViewModels;
@using sso.mms.portal.ext.ViewModels;
@using sso.mms.portal.ext.Services;
@using sso.mms.helper.Configs;
@using sso.mms.helper.Data;
@using sso.mms.login.Services;

@inject CertificateService certificateservice;
@inject ILocalStorageService _localstorage;
@inject IJSRuntime JSRuntime;
@inject NavigationManager Navigation;
@inject SweetAlertService Swal;
@inject UserRoleService userRoleService

<style>
    .btn-back {
        background: #FFFFFF;
        border: 2px solid #334396;
        border-radius: 4px;
        font-family: 'Noto Sans Thai';
        font-style: normal;
        font-weight: 600;
        font-size: 18px;
        line-height: 144%;
        color: #334396;
        width: 173px;
        height: 51px
    }
    @@media only screen and (min-width: 1000px) {
        .pm-xl {
            position: fixed;
            width: 100%;
            z-index: 1000;
        }
    }

    @@media only screen and (max-width: 1000px) {
        .position-class {
            position: relative
        }
    }

    @@media only screen and (max-width: 400px) {
        .container {
            width: 100%;
            padding-right: var(--bs-gutter-x,.15rem);
            padding-left: var(--bs-gutter-x,.15rem);
            margin-right: auto;
            margin-left: auto;
        }

        .px-4 {
            padding-right: 0rem !important;
            padding-left: 0rem !important;
        }
    }

    .swal2-container.swal2-center > .swal2-popup {
        grid-column: 2;
        grid-row: 2;
        align-self: center;
        justify-self: center;
        border-radius: 16px;
    }

    .swal2-title {
        position: relative;
        max-width: 100%;
        margin: 0;
        padding: 0.8em 1em 0;
        color: inherit;
        font-size: 1.875em;
        font-weight: 600;
        text-align: center;
        text-transform: none;
        word-wrap: break-word;
        color: #334396;
        font-family: Noto Sans Thai;
    }

    .swal2-styled.swal2-confirm {
        border: 0;
        color: #fff;
        font-size: 1em;
        border-radius: 4px;
        background: #334396;
        width: 150px;
    }

</style>

<NavbarLoginSSO PageCode="HOP0701" Fname="@getUser?.FirstName" Lname="@getUser?.LastName" />


<div class="bg-color">
    <MediaStyle></MediaStyle>
    <div class="container pt-5">
        <div class="container">
            <div class="card-body">
                <div class="card-title">
                    <div class="text-title p-2">บัตรรับรองการขึ้นทะเบียน</div>
                </div>
                <div class="container mt-3 px-4">
                    <div class="row px-2">
                        <div class="col-sm-0 cool-md-0 col-lg-4 col-xl-4"></div>
                        <div class="col-sm-12 cool-md-6 col-lg-4 col-xl-4 text-cer d-flex justify-content-center"> บัตรรับรองการขึ้นทะเบียน </div>
                    </div>
                    <div class="container d-flex align-content-center justify-content-center mt-2 pb-5">
                        <div class="bg-upload-report d-flex justify-content-center align-content-center align-items-center">
                            <label style="width:100%; height: 100%" for="input-file">
                                @if (permit.isRoleUpdate == true)
                                {
                                    <InputFile id="input-file" class="custom-input-file " style="display:none" type="file" OnChange="@LoadImage" />
                                }

                                @if (FileName != "")
                                {

                                    <img class="rounded shadow-sm" style="width:100%; height:100%; cursor: pointer" src="@ImageUri" />
                                }
                                else
                                {
                                    <svg style="cursor: pointer" class="bg-upload-certificate justify-content-center align-content-center align-items-center" width="407" height="359" viewBox="0 0 407 359" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                                        <rect width="407" height="359" fill="url(#pattern0)" fill-opacity="0.3" />
                                        <defs>
                                            <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                                <use xlink:href="#image0_942_15826" transform="matrix(0.00195312 0 0 0.00221427 0 -0.0668524)" />
                                            </pattern>
                                            <image id="image0_942_15826" width="512" height="512" xlink:href="data:image/png;base64," />
                                        </defs>
                                    </svg>
                                }
                            </label>
                        </div>
                    </div>
                    <div class="row px-2">
                        <div class="col-sm-0 cool-md-0 col-lg-4 col-xl-4"></div>
                        <div class="col-sm-12 cool-md-6 col-lg-4 col-xl-4"></div>
                        <div class="col-sm-12 cool-md-6 col-lg-4 col-xl-4 d-flex justify-content-end">
                            @if (btnStatus == true)
                            {
                                <button class="btn btn-back btn-secondary" @onclick=@GoBack><i class="fa-solid fa-circle-chevron-left"></i>&nbsp; ย้อนกลับ</button>

                            }
                            &nbsp;
                            @if (permit.isRoleUpdate == true)
                            {
                                
                                <button class="btn btn-primary btn-cer-upload pl-2 mb-4" @onclick=OnUpload>
                                    <i class="fa-solid fa-cloud-arrow-up" style="color:white"></i> &nbsp;
                                    <span class="text-upload">บันทึก</span>
                                </button>
                            }

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="pt-4">
        <Footer></Footer>
    </div>
</div>



@if (DialogIsOpen)
{
    <ModalSaveChange OnClose="@OnDialogClose" title="ยืนยันการบันทึกใช่หรือไม่" description="">
    </ModalSaveChange>
}
@if (isloading == true)
{
    <style>body { overflow: hidden; }</style><LoadingPage></LoadingPage>
}
@code {
    public bool isloading = false;
    private bool DialogIsOpen = false;
    [Parameter]
    [SupplyParameterFromQuery(Name = "token")]
    public string? shortToken { get; set; } = "";

    public IBrowserFile CertificateFile;
    public string FileName = "";
    public string ImageUri = "";
    public string imgpreview = "";
    public ResponseUpload responseUpload { get; set; }
    public CertificateT rescert { get; set; }
    public string? prefix;
    public string url = "";
    public string? env = ConfigureCore.ConfigENV;
    private ResponseLogin getUser { get; set; } = null!;
    private string? getusername { get; set; }
    public string? hospitalCode { get; set; }
    public int? getHospitalMId;
    public UserRole userRole;
    public MenuPerMit permit = new MenuPerMit();
    public string? Img = "";
    public bool btnStatus = false;

    protected override async Task OnInitializedAsync()
    {
        isloading = true;
        url = ConfigureCore.redirectPortalExt;

        getUser = await _localstorage.GetItemAsync<ResponseLogin>("userdata");
        getHospitalMId = getUser.HospitalMId;
        getusername = await _localstorage.GetItemAsync<String>("username");
        hospitalCode = await _localstorage.GetItemAsync<String>("hospitalMCode");
        rescert = await certificateservice.GetCertificate(hospitalCode);
        if (rescert.UploadImagePath != null)
        {
            Img = rescert.UploadImagePath;

        }

        FileName = Img;
        ImageUri = Img;
        Console.WriteLine("resert : " + rescert);
        Console.WriteLine("FileName :" + FileName);
        userRole = await _localstorage.GetItemAsync<UserRole>("userRole");
        if (userRole != null && userRole.role != null)
        {
            try
            {
                permit = await userRoleService.GetUserAuth(userRole, "PHM003", "PORTAL-HOS");
            }
            catch (Exception ex)
            {
                Console.WriteLine(ex);
            }
        }
        isloading = false;
        StateHasChanged();

    }

    private async Task OnDialogClose(bool isOk)
    {
        if (isOk)
        {
            isloading = true;
            RequestCertificate certificate = new RequestCertificate()
                {
                    File = CertificateFile,
                };
            var response = await certificateservice.AddCertificate(certificate, getusername, hospitalCode);
            if (response.issucessStatus == true)
            {
                isloading = false;
                StateHasChanged();
                await Swal.FireAsync(
                    "บันทึกสำเร็จ",
                    null,
                    SweetAlertIcon.Success
                    );
                btnStatus = true;
            }
            else
            {
                isloading = false;
                StateHasChanged();
                await Swal.FireAsync(
                    "บันทึกไม่สำเร็จ",
                    null,
                    SweetAlertIcon.Error
                    );
            }
        }
        DialogIsOpen = false;
        StateHasChanged();
    }

    private async void GoBack()
    {
        Navigation.NavigateTo($"{url}/portal?token={shortToken}");
    }

    private async void OnUpload()
    {
        DialogIsOpen = true;
        StateHasChanged();
    }
    public async Task LoadImage(InputFileChangeEventArgs inputFileChangeEventArgs)
    {
        FileName = inputFileChangeEventArgs.File.Name;
        CertificateFile = inputFileChangeEventArgs.File;
        var image = await inputFileChangeEventArgs.File.RequestImageFileAsync("image/png", 600, 600);
        using Stream imageStream = image.OpenReadStream(1024 * 1024 * 10);
        using MemoryStream ms = new();
        await imageStream.CopyToAsync(ms);
        ImageUri = $"data:image/png;base64,{Convert.ToBase64String(ms.ToArray())}";
        StateHasChanged();
    }



}